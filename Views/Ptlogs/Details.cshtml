    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Ptltype)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ptltype)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PtlDate)
